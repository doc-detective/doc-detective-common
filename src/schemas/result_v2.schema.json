{
  "title": "result",
  "type": "object",
  "description": "Results about how a spec, test, or action performed.",
  "properties": {
    "status": {
      "type": "string",
      "description": "Returned status.",
      "enum": ["PASS", "FAIL", "WARNING", "SKIPPED"]
    },
    "description": {
      "type": "string",
      "description": "Description of the status."
    }
  },
  "readOnly": true,
  "required": ["status", "description"],
  "additionalProperties": false,
  "examples": [
    {
      "status": "PASS",
      "description": "Opened URL."
    },
    {
      "status": "FAIL",
      "description": "Couldn't open URL."
    },
    {
      "status": "WARNING",
      "description": "Opened URL with implied HTTPS protocol. Add the intended protocol to your `goTo` action's `url` field."
    },
    {
      "status": "SKIPPED",
      "description": "The current environment doesn't support the specified contexts."
    }
  ]
}
